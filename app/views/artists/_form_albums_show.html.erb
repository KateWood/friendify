<h1 class="page-header"><%= @album.name %></h1>
 <%# @album.artists.each do |artist| %>
  <h3><%#= artist.name %></h3>
  <%#=@album.inspect%>
  <ul>
    <% @album.tracks.each do |track| %>
      <li>
        <%= track.track_number %> |
        <%= track.artists[0].name %> |
        <%= track.name %> |
        <%= track.duration_ms %> |
        <audio controls style="width:45px">
          <source src="<%= track.preview_url %>">
        </audio>
      </li>
    <% end %>
  </ul>
 <%#  end %>

 <%= link_to "Back", :back %>