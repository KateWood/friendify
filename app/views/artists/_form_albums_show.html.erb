<h1 class="page-header"><%= @album.name %></h1>
<h3>by <%= @album.artists[0].name %></h3>
 

 <%# @album.artists.each do |artist| %>
  <h3><%#= artist.name %></h3>
  <%#=@album.inspect%>
  <table class="table table-hover">
    <% @album.tracks.each do |track| %>
      <tr>
        <td><%= track.track_number %></td>
        <td><%= track.name %></td>
        <td><%= track.duration_ms %></td>
        <td><%= link_to 'Preview Track', track.preview_url %></td>
      </tr>
      
    <% end %>  
  </table>

  
